{
  "name": "gdbots/pbjx-php",
  "homepage": "https://github.com/gdbots/pbjx-php",
  "description": "Messaging tools for pbj-php.",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "php": ">=5.4",
    "gdbots/pbj-php": "~0.2.1",
    "psr/log": "~1",
    "react/promise": "~2.2",
    "symfony/event-dispatcher": "~2.6"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.3"
  },
  "suggest": {
    "ext-gearman": "Allows you to use GearmanTransport and GearmanConsumer for distributed processing."
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  },
  "autoload": {
    "psr-0": {
      "Gdbots\\Pbjx\\": "src/"
    }
  }
}
