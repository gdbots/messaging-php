{
  "name": "gdbots/pbjx",
  "description": "Messaging tools for pbj.",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "php": ">=5.4",
    "gdbots/pbj": "dev-marshalers",
    "psr/log": "~1",
    "react/promise": "~2.2",
    "symfony/event-dispatcher": "~2.6"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.3"
  },
  "suggest": {
    "ext-gearman": "Allows you to use GearmanTransport and GearmanConsumer for distributed processing."
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.2.x-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "Gdbots\\Pbjx\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gdbots\\Tests\\Pbjx\\": "tests/"
    }
  },
  "replace": {
    "gdbots/pbjx-php": "self.version"
  }
}
